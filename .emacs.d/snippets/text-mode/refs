# -*- mode: snippet; require-final-newline: nil -*-
# name: refs
# key: re
# binding: direct-keybinding
# --
refs #`(let ((br (shell-command-to-string "git branch"))
             (re "\\(dev\\|bug\\)\\([0-9]*\\)"))
         (when (string-match re (cond ((stringp br) br) (t "")))
           (concat (match-string 2 br) " ")))`$0